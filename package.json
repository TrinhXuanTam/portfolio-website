    "format": "prettier --write \"**/*.{js,jsx,tsx,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "codegen": "graphql-codegen --config codegen.yml --watch src/**/api/graphql/**/*.ts"
  },
