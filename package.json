    "@typescript-eslint/parser": "^5.10.0",
    "concurrently": "^8.2.2",
    "eslint": "^8",
    "eslint-config-next": "^14.0.4",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^2.5.1",
    "typescript": "^5.3.3"
